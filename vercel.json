{
  "buildCommand": "cd red-sovereign && npm run lint && npm run typecheck && npm run build",
  "installCommand": "cd red-sovereign && npm install",
  "outputDirectory": "red-sovereign/.next",
  "framework": "nextjs",
  "functions": {
    "red-sovereign/src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}